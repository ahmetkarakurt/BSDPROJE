        <Sql>SELECT        dbo.TBLCAHAR.SUBE_KODU, dbo.TBLCAHAR.CARI_KOD, dbo.TBLCAHAR.TARIH, dbo.TBLCAHAR.VADE_TARIHI, dbo.TBLCAHAR.BELGE_NO,  REPLACE(REPLACE(REPLACE(dbo.TBLCAHAR.ACIKLAMA,'Ý','İ'),'Ð','Ğ'),'Þ','Ş') AS ACIKLAMA  , dbo.TBLCAHAR.HKA, dbo.TBLCAHAR.BORC, 
                         dbo.TBLCAHAR.ALACAK, dbo.TBLCAHAR.BAKIYE, dbo.TBLCAHAR.DOVIZ_TURU, dbo.TBLCAHAR.DOVIZ_TUTAR, dbo.TBLCAHAR.RAPOR_KODU, dbo.TBLCAHAR.F9SC, dbo.TBLCAHAR.HAREKET_TURU, 
                         dbo.TBLCAHAR.MIKTAR, dbo.TBLCAHAR.ILAVE_RAPOR_KODU, dbo.TBLCAHAR.UPDATE_KODU, dbo.TBLCAHAR.KAPATILMIS_TUTAR, dbo.TBLCAHAR.ODEME_GUNU, dbo.TBLCAHAR.FIRMA_DOVIZ_TIPI, 
                         dbo.TBLCAHAR.FIRMA_DOVIZ_TUTARI, dbo.TBLCAHAR.PLASIYER_KODU, dbo.TBLCAHAR.ENT_REF_KEY, dbo.TBLCAHAR.INC_KEY_NUMBER, dbo.TBLCAHAR.RAPOR_KODU2, dbo.TBLCAHAR.DUZELTMETARIHI, 
                         dbo.TBLCAHAR.S_YEDEK1, dbo.TBLCAHAR.S_YEDEK2, dbo.TBLCAHAR.F_YEDEK1, dbo.TBLCAHAR.F_YEDEK2, dbo.TBLCAHAR.C_YEDEK1, dbo.TBLCAHAR.C_YEDEK2, dbo.TBLCAHAR.B_YEDEK1, 
                         dbo.TBLCAHAR.I_YEDEK1, dbo.TBLCAHAR.L_YEDEK1, dbo.TBLCAHAR.D_YEDEK1, dbo.TBLCAHAR.PROJE_KODU, dbo.TBLCAHAR.ONAYTIPI, dbo.TBLCAHAR.ONAYNUM, dbo.TBLCAHAR.BAGLANTI_NO, 
                         dbo.TBLCASABIT.SUBE_KODU AS Expr1, dbo.TBLCASABIT.ISLETME_KODU, dbo.TBLCASABIT.CARI_KOD AS Expr2, dbo.TBLCASABIT.CARI_TEL, dbo.TBLCASABIT.CARI_IL, dbo.TBLCASABIT.ULKE_KODU, 
                         REPLACE(REPLACE(REPLACE(dbo.TBLCASABIT.CARI_ISIM,'Ý','İ'),'Ð','Ğ'),'Þ','Ş') as CARI_ISIM , dbo.TBLCASABIT.CARI_TIP, dbo.TBLCASABIT.GRUP_KODU, dbo.TBLCASABIT.RAPOR_KODU1, dbo.TBLCASABIT.RAPOR_KODU2 AS Expr3, dbo.TBLCASABIT.RAPOR_KODU3, 
                         dbo.TBLCASABIT.RAPOR_KODU4, dbo.TBLCASABIT.RAPOR_KODU5, dbo.TBLCASABIT.CARI_ADRES, dbo.TBLCASABIT.CARI_ILCE, dbo.TBLCASABIT.VERGI_DAIRESI, dbo.TBLCASABIT.VERGI_NUMARASI, 
                         dbo.TBLCASABIT.FAX, dbo.TBLCASABIT.POSTAKODU, dbo.TBLCASABIT.DETAY_KODU, dbo.TBLCASABIT.NAKLIYE_KATSAYISI, dbo.TBLCASABIT.RISK_SINIRI, dbo.TBLCASABIT.TEMINATI, 
                         dbo.TBLCASABIT.CARISK, dbo.TBLCASABIT.CCRISK, dbo.TBLCASABIT.SARISK, dbo.TBLCASABIT.SCRISK, dbo.TBLCASABIT.CM_BORCT, dbo.TBLCASABIT.CM_ALACT, dbo.TBLCASABIT.CM_RAP_TARIH, 
                         dbo.TBLCASABIT.KOSULKODU, dbo.TBLCASABIT.ISKONTO_ORANI, dbo.TBLCASABIT.VADE_GUNU, dbo.TBLCASABIT.LISTE_FIATI, dbo.TBLCASABIT.ACIK1, dbo.TBLCASABIT.ACIK2, dbo.TBLCASABIT.ACIK3, 
                         dbo.TBLCASABIT.M_KOD, dbo.TBLCASABIT.DOVIZ_TIPI, dbo.TBLCASABIT.DOVIZ_TURU AS Expr4, dbo.TBLCASABIT.HESAPTUTMASEKLI, dbo.TBLCASABIT.DOVIZLIMI, dbo.TBLCASABIT.UPDATE_KODU AS Expr5, 
                         dbo.TBLCASABIT.PLASIYER_KODU AS Expr6, dbo.TBLCASABIT.LOKALDEPO, dbo.TBLCASABIT.EMAIL, dbo.TBLCASABIT.WEB, dbo.TBLCASABIT.KURFARKIBORC, dbo.TBLCASABIT.KURFARKIALAC, 
                         dbo.TBLCASABIT.S_YEDEK1 AS Expr7, dbo.TBLCASABIT.S_YEDEK2 AS Expr8, dbo.TBLCASABIT.F_YEDEK1 AS Expr9, dbo.TBLCASABIT.F_YEDEK2 AS Expr10, dbo.TBLCASABIT.C_YEDEK1 AS Expr11, 
                         dbo.TBLCASABIT.C_YEDEK2 AS Expr12, dbo.TBLCASABIT.B_YEDEK1 AS Expr13, dbo.TBLCASABIT.I_YEDEK1 AS Expr14, dbo.TBLCASABIT.L_YEDEK1 AS Expr15, dbo.TBLCASABIT.FIYATGRUBU, 
                         dbo.TBLCASABIT.KAYITYAPANKUL, dbo.TBLCASABIT.KAYITTARIHI, dbo.TBLCASABIT.DUZELTMEYAPANKUL, dbo.TBLCASABIT.DUZELTMETARIHI AS Expr16, dbo.TBLCASABIT.ODEMETIPI, 
                         dbo.TBLCASABIT.ONAYTIPI AS Expr17, dbo.TBLCASABIT.ONAYNUM AS Expr18, dbo.TBLCASABIT.MUSTERIBAZIKDV, dbo.TBLCASABIT.AGIRLIK_ISK, dbo.TBLCASABIT.CARI_TEL2, dbo.TBLCASABIT.CARI_TEL3, 
                         dbo.TBLCASABIT.FAX2, dbo.TBLCASABIT.GSM1, dbo.TBLCASABIT.GSM2, dbo.TBLCASABIT.GEKAPHESAPLANMASIN, dbo.TBLCASABIT.ONCEKI_KOD, dbo.TBLCASABIT.SONRAKI_KOD, 
                         dbo.TBLCASABIT.SONCARIKODU, dbo.TBLCASABIT.TESLIMCARIBAGLIMI, dbo.TBLCASABIT.BAGLICARIKOD, REPLACE(REPLACE(REPLACE(dbo.TBLCARIPLASIYER.PLASIYER_ACIKLAMA,'Ý','İ'),'Ð','Ğ'),'Þ','Ş') AS PLASIYER_ACIKLAMA ,
						 						 CASE
												     WHEN dbo.TBLCAHAR.HAREKET_TURU='A' THEN 'DEVIR'
    WHEN dbo.TBLCAHAR.HAREKET_TURU='B' THEN 'FATURA'
	 WHEN dbo.TBLCAHAR.HAREKET_TURU='C' THEN 'IADE FATURASI'
	  WHEN dbo.TBLCAHAR.HAREKET_TURU='D' THEN 'KASA'
	   WHEN dbo.TBLCAHAR.HAREKET_TURU='E' THEN 'MUSTERI SENETI'
	    WHEN dbo.TBLCAHAR.HAREKET_TURU='F' THEN 'BORC SENETI'
		 WHEN dbo.TBLCAHAR.HAREKET_TURU='G' THEN 'MUSTERI CEKI'
		  WHEN dbo.TBLCAHAR.HAREKET_TURU='H' THEN 'BORC CEKI'
		   WHEN dbo.TBLCAHAR.HAREKET_TURU='I' THEN 'PROTESTOLU SENET'
		  WHEN dbo.TBLCAHAR.HAREKET_TURU='J' THEN 'KARSILIKSIZ CEK'
		  WHEN dbo.TBLCAHAR.HAREKET_TURU='K' THEN 'DEKONT'
		    WHEN dbo.TBLCAHAR.HAREKET_TURU='L' THEN 'MUHTELIF'


    ELSE 'DIGER' END AS HARAKETTURU 
FROM            dbo.TBLCASABIT LEFT OUTER JOIN
                         dbo.TBLCARIPLASIYER ON dbo.TBLCASABIT.PLASIYER_KODU = dbo.TBLCARIPLASIYER.PLASIYER_KODU RIGHT OUTER JOIN
                         dbo.TBLCAHAR ON dbo.TBLCASABIT.CARI_KOD = dbo.TBLCAHAR.CARI_KOD
WHERE        (NOT (dbo.TBLCAHAR.HAREKET_TURU IN ('B', 'C', 'L', 'A')))
</Sql>
