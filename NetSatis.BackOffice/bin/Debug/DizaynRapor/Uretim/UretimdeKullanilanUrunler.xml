    <SqlDataSource Name="SQL veri kaynağı 1" ComponentName="dashboardSqlDataSource1">
      <Connection Name="eenwindConnection" FromAppConfig="true" />
      <Query Type="CustomSqlQuery" Name="Query">
        <Sql>SELECT        TOP (100) PERCENT dbo.TBLSTOKURM.MAMUL_KODU, dbo.TBLSTOKURM.HAM_KODU, dbo.TBLSTOKURM.MIKTAR, dbo.TBLSTOKURM.STOK_MALIYET, TBLSTSABIT_1.STOK_ADI, 
                         dbo.TBLSTSABIT.STOK_ADI AS MAMUL_ADI, TBLSTSABIT_1.GRUP_KODU, TBLSTSABIT_1.OLCU_BR1
FROM            dbo.TBLSTOKURM LEFT OUTER JOIN
                         dbo.TBLSTSABIT ON dbo.TBLSTOKURM.MAMUL_KODU = dbo.TBLSTSABIT.STOK_KODU LEFT OUTER JOIN
                         dbo.TBLSTSABIT AS TBLSTSABIT_1 ON dbo.TBLSTOKURM.HAM_KODU = TBLSTSABIT_1.STOK_KODU
WHERE        (dbo.TBLSTOKURM.GEC_FLAG = 0) AND (dbo.TBLSTOKURM.GEC_SON_DURUM &lt;&gt; 'K') AND (ISNULL(dbo.TBLSTOKURM.MAMYAPKOD, '') = '')
ORDER BY dbo.TBLSTOKURM.INCKEYNO</Sql>
      </Query>
